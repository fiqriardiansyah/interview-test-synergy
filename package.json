{
  "name": "synergy-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "format": "prettier --write --ignore-path=.gitignore .",
    "format:check": "prettier --check --ignore-path=.gitignore .",
    "deploy:vercel": "vercel",
    "commit": "npm run lint & git-cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/node": "20.8.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "eslint": "8.52.0",
    "eslint-config-next": "14.0.0",
    "framer-motion": "^9.1.7",
    "next": "14.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "autoprefixer": "^10.4.16",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "react-icons": "^4.11.0",
    "tailwindcss": "^3.3.5",
    "vercel": "^30.2.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,html,css,json}": "prettier --write --ignore-unknown"
  }
}
